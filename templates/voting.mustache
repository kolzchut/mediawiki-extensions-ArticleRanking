<div class="ranking-section hidden-print noprint">
	<div class="header">{{ section-title }}</div>
	<div class="content">
		<div class="buttons">
			<button class="btn btn-secondary ranking-btn yes" type="button" aria-label="{{ yes }}" title="{{ yes }}"><i class="kzi kzi-thumb-up"></i></button>
			<button class="btn btn-secondary ranking-btn no" type="button" aria-label="{{ no }}" title="{{ no }}"><i class="kzi kzi-thumb-down"></i></button>
		</div>
		<div class="voting-messages"></div>
		{{#is-captcha-enabled}}
			<div class="h-captcha"
				 data-sitekey="{{ siteKey }}"
				 data-size="invisible"
				 data-callback="verifyRankingCaptcha"
				 data-error-callback="handleRankingCaptchaError"
				 data-close-callback="handleRankingCaptchaError"
			></div>
		{{/is-captcha-enabled}}
	</div>
</div>
