<div class="ranking-section hidden-print noprint">
	<div class="sub-section sub-section1">
		<h2>{{ section1title }}</h2>
		<div class="buttons">
			<button class="btn btn-secondary ranking-btn yes" type="button">{{ yes }}</button>
			<button class="btn btn-secondary ranking-btn no" type="button">{{ no }}</button>
		</div>

		<div class="voting-messages"></div>
		{{#is-captcha-enabled}}
		<div class="g-recaptcha"
			 data-sitekey="{{ siteKey }}"
			 data-callback="verifyRankingCaptcha"
			 data-error-callback="handleRankingCaptchaError"
			 data-size="invisible">
		</div>
		{{/is-captcha-enabled}}
	</div>

	{{{ section2 }}}
</div>
