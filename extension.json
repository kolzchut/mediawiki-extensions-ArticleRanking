{
	"name": "ArticleRanking",
	"version": "2.0.1",
	"author": [
		"Tal Koren (Interjet, Kol Zchut)",
		"Dror S. [FFS] ([https://www.kolzchut.org.il Kol-Zchut])"
	],
	"descriptionmsg": "articleranking-desc",
	"url": "https://github.com/kolzchut/mediawiki-extensions-ArticleRanking",
	"license-name": "MIT",
	"type": "other",
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\ArticleRanking\\": "includes/"
	},
	"APIModules": {
		"rank-vote": "MediaWiki\\Extension\\ArticleRanking\\Api\\ApiVote",
		"rank-votes": "MediaWiki\\Extension\\ArticleRanking\\Api\\ApiGetVotes"
	},
	"SpecialPages": {
		"ArticleRanking": "MediaWiki\\Extension\\ArticleRanking\\SpecialArticleRankingReport"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "WikiRights/ArticleRanking"
	},
	"ResourceModules": {
		"ext.articleRanking": {
			"scripts": "modules/article-ranking.script.js",
			"styles": "modules/article-ranking.style.less",
			"dependencies": [
				"mediawiki.jqueryMsg",
				"mediawiki.api"
			],
			"messages": [
				"ranking-vote-success",
				"ranking-vote-fail",
				"ranking-cr-form-title"
			]
		},
		"ext.articleRanking.changeRequest": {
			"scripts": "modules/articleRanking.changeRequest.js",
			"dependencies": ["ext.wr.ShareBar.js", "mediawiki.Uri"]
		}
	},
	"Hooks": {
		"BeforePageDisplay": "MediaWiki\\Extension\\ArticleRanking\\Hooks::onBeforePageDisplay",
		"LoadExtensionSchemaUpdates": "MediaWiki\\Extension\\ArticleRanking\\Hooks::onLoadExtensionSchemaUpdates",
		"ResourceLoaderGetConfigVars": "MediaWiki\\Extension\\ArticleRanking\\Hooks::onResourceLoaderGetConfigVars"
	},
	"MessagesDirs": {
		"ArticleRanking": [
			"i18n"
		]
	},
	"GroupPermissions": {
		"sysop": {
			"articleranking-view-report": true
		}
	},
	"config": {
		"ArticleRankingConfig": {
			"changerequest": {
				"url": "/forms/ChangeRequest/",
				"width": 750,
				"height": 650
			},
			"trackClicks": true
		},
		"ArticleRankingCaptcha": {
			"siteKey": "",
			"secret": ""
		}
	},
	"manifest_version": 1
}
